ypeof(argument.value));\n\n      this.bitfield_update(argument); //console.log(\"bitfield_inverse\", argument.textContent)\n\n      let value = 0; // this.items = 7\n      //console.log(this.items)\n\n      this.bitwidget_values.forEach(function (item, index, array) {\n        value += 2 ** index * item.value;\n      }); // console.log(value)\n\n      if (value != 0) {\n        if (this.bitwidget_state) {\n          this.calculator_input = this.calculator_input_prev;\n        }\n\n        this.insert(value, true);\n      } else {\n        this.calculator_input = this.calculator_input_prev;\n      } //console.log(value);\n\n\n      var tmp = [];\n      this.bitwidget_values.forEach(function (item, index, array) {\n        tmp.push(item.value);\n      }); //console.log(tmp);\n    },\n\n    bitfieldwidget_clear(selfo) {\n      selfo.bitwidget_values.forEach(function (item, index, array) {\n        //console.log(item.textContent)\n        //item.value = 0;\n        selfo.bitwidget_values[index].value = 0;\n        var q = document.getElementById(\"calculator-bit\" + index);\n        q.value = 0; // selfo.bitfield_update(item);\n\n        selfo.bitfield_update(q);\n      });\n    },\n\n    insert(num, bitwidget_state_l = false) {\n      this.bitwidget_state = bitwidget_state_l;\n\n      if (this.bitwidget_state === false) {\n        this.bitfieldwidget_clear(this);\n      }\n\n      this.calculator_input_prev = this.calculator_input;\n      this.calculator_input = this.calculator_input + num;\n    },\n\n    clean() {\n      this.bitwidget_state = false;\n      this.bitfieldwidget_clear(this);\n      this.calculator_input_prev = this.calculator_input;\n      this.calculator_input = \"\";\n    },\n\n    back() {\n      // console.log(\"this\", this)\n      this.bitwidget_state = false;\n      this.bitfieldwidget_clear(this);\n      var exp = this.calculator_input;\n      this.calculator_input_prev = this.calculator_input;\n      this.calculator_input = exp.substring(0, exp.length - 1);\n    },\n\n    equal() {\n      this.bitwidget_state = false;\n      this.bitfieldwidget_clear(this);\n      var exp = this.calculator_input;\n      this.calculator_input_prev = this.calculator_input;\n\n      if (exp) {\n        this.calculator_input = eval(exp);\n      }\n    }\n\n  },\n\n  mounted() {}\n\n};","map":{"version":3,"mappings":"AAsHC,eAAe;EACdA,IAAI,EAAE,YADQ;;EAEd;EACA;EACA;EACA;EACA;EACA;EACAC,IAAI,GAAG;IACN,IAAIC,gBAAe,GAAI,IAAIC,KAAJ,EAAvB;;IACA,KAAI,IAAIC,IAAI,CAAZ,EAAeA,IAAI,EAAnB,EAAwBA,CAAC,EAAzB,EAA6B;MAC5BF,gBAAgB,CAACG,OAAjB,CAAyB;QAAEC,KAAK,EAAE;MAAT,CAAzB,EAD4B,CAE5B;IACD,CALM,CAMN;;;IACA,OAAO;MACNJ,gBAAgB,EAAEA,gBADZ;MAENK,eAAe,EAAE,KAFX;MAGNC,gBAAgB,EAAE,EAHZ;MAINC,qBAAqB,EAAE;IAJjB,CAAP;EAMA,CArBa;;EAsBdC,OAAO,EAAE;IACRC,eAAe,CAACC,QAAD,EAAW;MACzB;MACA;MACA;MACA;MACA,IAAGA,QAAQ,CAACN,KAAT,IAAkB,GAArB,EAA0B;QACzBM,QAAQ,CAACC,YAAT,CAAsB,OAAtB,EAA+B,2DAA/B,EADyB,CACoE;MAC9F,CAFA,MAEO;QACND,QAAQ,CAACC,YAAT,CAAsB,OAAtB,EAA+B,wDAA/B,EADM,CACoF;MAC3F,CATyB,CAUzB;MACA;;IACA,CAbO;;IAcRC,gBAAgB,CAACF,QAAD,EAAW;MAE1B;MACA;MACA;MAEA;MACA,IAAGA,QAAQ,CAACN,KAAT,IAAkB,CAArB,EAAwB;QACvB;QACA;QACAM,QAAQ,CAACN,KAAT,GAAiB,CAAjB;MACD,CAJA,MAIO;QACN;QACA;QACAM,QAAQ,CAACN,KAAT,GAAiB,CAAjB;MACD;;MAEA,KAAKJ,gBAAL,CAAsBU,QAAQ,CAACG,WAA/B,EAA4CT,KAA5C,GAAoDU,QAAQ,CAACJ,QAAQ,CAACN,KAAV,CAA5D,CAjB0B,CAmB1B;MACA;;MACA,KAAKK,eAAL,CAAqBC,QAArB,EArB0B,CAsB1B;;MACA,IAAIN,KAAI,GAAI,CAAZ,CAvB0B,CAyB1B;MACA;;MAEA,KAAKJ,gBAAL,CAAsBe,OAAtB,CAA8B,UAASC,IAAT,EAAeC,KAAf,EAAsBC,KAAtB,EAA6B;QAC1Dd,KAAI,IAAM,KAAKa,KAAN,GAAgBD,IAAI,CAACZ,KAA9B;MACA,CAFD,EA5B0B,CA+B1B;;MACA,IAAGA,KAAI,IAAK,CAAZ,EAAe;QACd,IAAG,KAAKC,eAAR,EAAyB;UACxB,KAAKC,gBAAL,GAAwB,KAAKC,qBAA7B;QACD;;QACA,KAAKY,MAAL,CAAYf,KAAZ,EAAmB,IAAnB;MACD,CALA,MAKO;QACN,KAAKE,gBAAL,GAAwB,KAAKC,qBAA7B;MACD,CAvC0B,CAyC1B;;;MACA,IAAIa,GAAE,GAAI,EAAV;MACA,KAAKpB,gBAAL,CAAsBe,OAAtB,CAA8B,UAASC,IAAT,EAAeC,KAAf,EAAsBC,KAAtB,EAA6B;QAAEE,GAAG,CAACC,IAAJ,CAASL,IAAI,CAACZ,KAAd;MAAsB,CAAnF,EA3C0B,CA4C1B;IACA,CA3DO;;IA4DRkB